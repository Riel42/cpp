#include <iostream>

using namespace std;

int main(){
	
	int N, M;

	N = 0;

	N++;
	cout << N << endl;	
	
	N+=100;
	cout << N << endl;

	N*=2;
	cout << N << endl;

	//M++ --> M = M + 1
	//++M --> M = 1 + M

	cout << endl;

	for(M = 0; M <= 3; ++M){
		cout << M << endl;
	}	
	
	cout << endl;

	for(M = 0; M <= 3; M++){
		cout << M << endl;
	}

	return 0;
}
